variable "aws_region" {
    type = string
    default = "us-east-1"
  
}

variable "vpc_cidr" {
  description = "CIDR block for the VPC"
  # default     = "170.20.0.0/16"
}
variable "public_subnet" {
    type = list(object({
        cidr = string
        az = string  
    }))
  
}

variable "private_subnet" {
    type = list(object({
        cidr = string
        az = string  
    }))
  
}
